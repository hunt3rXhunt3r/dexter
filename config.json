{
"log": {
  "access": "/var/log/v2ray/access.log",
  "error": "/var/log/v2ray/error.log",
  "loglevel": "warning"
},
"inbounds": [{
  "port": 10035,
  "listen": "127.0.0.1",
  "protocol": "vmess",
  "settings": {
    "clients": [
      {
        "id": "GENERATED_UUID_CODE",
        "level": 1,
        "alterId": 64
      }
    ]
    },
    "streamSettings": {
      "network": "ws",
      "wsSettings": {
	"headers": {
          "Host": "SERVER_DOMAIN"
        },
        "path": "/"
      }
    }
  }],
  "outbounds": [{
    "protocol": "freedom",
    "settings": {}
  },{
    "protocol": "blackhole",
    "settings": {},
    "tag": "blocked"
  }],
  "routing": {
    "rules": [
      {
        "type": "field",
        "ip": ["geoip:private"],
        "outboundTag": "blocked"
      }
    ]
  }
}